CC = @CC@
VERSION = @VERSION@
CFLAGS = @CFLAGS@
LIBS = @LIBS@
FLAGS=$(CFLAGS) @DEFS@
INSTALL=@INSTALL@
prefix=@prefix@
bindir=$(prefix)@bindir@

all: 
	@cd libUseful-4; $(MAKE)
	$(CC) $(FLAGS) $(LIBS) -g -ogetlock main.c libUseful-4/libUseful-4.a

clean:
	@rm -f getlock libUseful-4/*.o libUseful-4/*.a libUseful-4/*.so

install:
	$(INSTALL) getlock $(bindir)

test:
	-echo "no tests"
